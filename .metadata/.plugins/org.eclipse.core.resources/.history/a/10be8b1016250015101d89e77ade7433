package com.banjo;

import java.awt.Color;
import java.awt.Graphics;

public class TrafficLight {
	boolean status=false;
	static int inteval=50;
	int count=0;
	public TrafficLight(boolean s){
		this.status=s;
	}
	public void run(){
		count++;
		if(count==10) {
			status=!status;
			count=0;
		}
	}
	public void paint(Graphics g){
		Color c = g.getColor();
		g.setColor(Color.BLACK);
		
		g.setColor(c);
	}
}
